{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "args": ["-remote=auto"],
            "rootPatterns": ["go.mod", ".vim/", ".git/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "completeUnimported": true,
                "deepCompletion": true
            }
        },
        "nim": {
            "command": "nimlsp",
            "filetypes": [ "nim" ],
            "trace.server": "verbose"
        }
    },

    "suggest.completionItemKindLabels": {
        "keyword":       "\uf1de",
        "variable":      "\ue79b",
        "value":         "\uf89f",
        "operator":      "\u03a8",
        "function":      "\u0192",
        "reference":     "\ufa46",
        "constant":      "\uf8fe",
        "method":        "\uf09a",
        "struct":        "\ufb44",
        "class":         "\uf0e8",
        "interface":     "\uf417",
        "text":          "\ue612",
        "enum":          "\uf435",
        "enumMember":    "\uf02b",
        "module":        "\uf40d",
        "color":         "\ue22b",
        "property":      "\ue624",
        "field":         "\uf9be",
        "unit":          "\uf475",
        "event":         "\ufacd",
        "file":          "\uf723",
        "folder":        "\uf114",
        "snippet":       "\ue60b",
        "typeParameter": "\uf728",
        "default":       "\uf29c"
    },
    "coc.preferences.currentFunctionSymbolAutoUpdate": false,
    "suggest.noselect": false,
    "suggest.minTriggerInputLength": 1,
    "suggest.snippetIndicator": "  üê®",
    "coc.preferences.formatOnType":true,
    "coc.preferences.formatOnSaveFiletypes": ["go", "lua", "nim"],
    "diagnostic.errorSign": "‚úò",
    "diagnostic.hintSign": "‚û§",
    "Lua.telemetry.enable": false
}
